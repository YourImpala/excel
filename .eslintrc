{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "require-jsdoc": "off",
    "linebreak-style": "off",
    "eol-last": "off",
    "indent": "off",
    "quotes": "off",
    "operator-linebreak": "off",
    "max-len": "off"
  },
  "extends": [
    "eslint:recommended",
    "google"
   // "plugin:security/recommended"
  ]
//  "plugins": [
//    "security"
//  ]
}